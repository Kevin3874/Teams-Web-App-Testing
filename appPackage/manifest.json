{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Voyce Global, Inc.",
        "websiteUrl": "https://web.voyceglobal.com/",
        "privacyUrl": "https://voyceglobal.com/privacy-policy.html",
        "termsOfUseUrl": "https://voyceglobal.com/terms-and-conditions.html"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Voyce-${{TEAMSFX_ENV}}",
        "full": "Voyce Global"
    },
    "description": {
        "short": "Voyce Connect Teams App",
        "full": "Voyce Global Teams App"
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [],
    "staticTabs": [
        {
            "entityId": "index0",
            "name": "Personal Tab",
            "contentUrl": "${{TAB_ENDPOINT}}/tab",
            "websiteUrl": "${{TAB_ENDPOINT}}/tab",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_DOMAIN}}"
    ],
    "showLoadingIndicator": false
}